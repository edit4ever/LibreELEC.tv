<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="@PKG_ADDON_ID@"
       name="@PKG_ADDON_NAME@"
       version="@PKG_ADDON_VERSION@"
       provider-name="@PKG_PROVIDER_NAME@">
  <requires>
    <import addon="xbmc.python" version="2.6.0"/>
    <import addon="script.module.dateutil" version="2.4.2"/>
    <import addon="script.module.xbmcswift2" version="2.4.0"/>
    <import addon="script.module.requests" version="2.9.1" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>@PKG_SHORTDESC@</summary>
    <description>@PKG_LONGDESC@</description>
    <disclaimer>@PKG_DISCLAIMER@</disclaimer>
    <platform>all</platform>
    <license>@PKG_LICENSE@</license>
    <assets>
        <icon>icon.png</icon>
        <fanart></fanart>
        <screenshot>resources/screenshot-01.jpg</screenshot>
        <screenshot>resources/screenshot-02.jpg</screenshot>
        <screenshot>resources/screenshot-03.jpg</screenshot>
    </assets>
    <news>@PKG_ADDON_NEWS@</news>
  </extension>
</addon>
